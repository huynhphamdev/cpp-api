{
  "name": "learning-platform-api",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start:dev": "npm run update:message && cross-env DEBUG=app nodemon ./bin/www",
    "start:prod": "npm run update:message && cross-env NODE_ENV=production pm2 start ./bin/www",
    "start:aws": "pm2-runtime ./bin/www",
    "deploy:prod": "npm run update:message && node scripts/deploy.js --server production",
    "stop:prod": "pm2 delete all",
    "update:message": "node scripts/update_messages.js",
    "eslint": "eslint --ext js .",
    "migrate": "sequelize db:migrate"
  },
  "dependencies": {
    "apisauce": "^1.1.1",
    "aws-sdk": "^2.973.0",
    "axios": "^0.19.2",
    "bcrypt": "^4.0.1",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "csv-stringify": "^5.6.0",
    "csvtojson": "^2.0.10",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "express-async-errors": "^3.1.1",
    "handlebars": "^4.7.7",
    "hbs": "~4.0.4",
    "http-errors": "~1.6.3",
    "i18n-js": "^3.7.1",
    "json2csv": "^5.0.5",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "minimist": "^1.2.5",
    "moment": "^2.27.0",
    "moment-timezone": "^0.5.31",
    "morgan": "~1.9.1",
    "node-cache": "^5.1.2",
    "nodemailer": "^6.4.18",
    "pg": "^8.2.1",
    "pm2": "^4.4.0",
    "qs": "^6.9.6",
    "require-dir": "^1.2.0",
    "sequelize": "^5.21.11",
    "sequelize-cli": "^5.5.1",
    "string-template": "^1.0.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.2",
    "eslint": "^7.22.0",
    "nodemon": "^2.0.4"
  }
}
